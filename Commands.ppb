alias restart {   
  .echo -a Restart IRC in 5 seconds.
  .timer 1 5 run mirc.exe
  .timer 1 5 exit -y
}
alias setting.change { hadd -m $replace($1,$chr(124),$chr(176)) $+(Settings.,$2) $3- | save.hashtables }
on *:NOTICE:*:*:{ 
  if ($nick == Q) && ($1-2 == AUTH'd successfully.) { mode $me +x | log Qauth Auth Successfully. }
  if ($nick == Q) || ($nick == L) { if ($hget(chanlev,chan)) { msg $hget(chanlev,chan) $styles($iif($nick == Q,Q,L) - $1-) | hfree chanlev } }
  if ($nick == R) { if $hget(Qrequest,chan) { msg $hget(Qrequest,chan) $styles(R) - $1- } }
  if ($1 === lag) && ($nick == $me) { if (!%connecting) { msg $hget(lagchan,chan) $styles(Lag: ( $calc($ticks - $2) $+ ms ))) | hfree lagchan } }
  if ($1 == say) { if (!%connecting) { if ($access($2,$nick) >= 50) { if (!%usersay) { set -u5 %usersay 1 | msg $2 $3- } } } }
}
on *:text:*:#:{ 
  if (!%connecting) { 
    if ($checkcmd($chan,$1)) || ($1 == !prefix) { 
      if (!%textflood. [ $+ [ $chan ] ]) {
        set -u2 %textflood. [ $+ [ $chan ] ] 1 
        signal cmd $chan $nick $1-
      } 
    } 
  } 
}
on *:text:*:?:{
  if (!%connecting) {
    if ($checkcmd($2,$1)) || ($1 == !prefix) {
      if (!%textflood. [ $+ [ $2 ] ]) {
        set -u2 %textflood. [ $+ [ $2 ] ] 1 
        signal cmd $2 $nick $1 $3- 
      } 
    } 
  } 
}
on *:signal:cmd:{
  if ($1 != $3) {
    if ($cmd($1,$3,help)) || ($cmd($1,$3,commands)) || ($cmd($1,$3,cmds)) { msg $1 Hilfe ? hmm hier: www.psh-com.ath.cx/bef.html }    
    elseif ($cmd($1,$3,Qrequest)) || ($cmd($1,$3,requestQ)) { if ($access($1,$2) >= 25) { hadd -m Qrequest chan $1 | msg r requestq $1 | msg $hget(Qrequest,chan) $styles(Request anforderung für Q läuft in $hget(Qrequest,chan)) } } 
    elseif ($cmd($1,$3,invite)) { if ($access($1,$2) >= 25) { invite $4 $1 | msg $1 $styles($4 Will das du $1 betrittst) } }    
    elseif ($cmd($1,$3,uptime)) { if (!$4) { msg $1 $styles(Uptime mIRC: $uptime(mirc,2) ; Server(Connected): $uptime(server,2) ; System(OS): $uptime(system,2)) } | elseif ($4 == mirc) || ($4 == system) || ($4 == server) { msg $1 $styles($iif($4 == mirc,mIRC,$iif($4 == server,Server,$iif($4 == system,System)))) $+ : $uptime($4,2)) } }   
    elseif ($cmd($1,$3,chanbot)) { msg $1 $styles(Channel-Bot: $iif($item($1,chanbot),$item($1,chanbot),-none-)) }   
    elseif ($cmd($1,$3,power)) {
      var %a = $scon(0),%b = 0,%c = 0,%i = 0,%j = 0,%x = $cid
      while (%a) { 
        %b = $calc(%b + $chan(0)) 
        scid $scid($scon(%a))
        var %o = $chan(0)
        while (%o) {
          if ($me isop $chan(%o)) { 
            inc %i $nick($chan(%o),0) 
            inc %c 
          } 
          inc %j $nick($chan(%o),0) 
          dec %o
        } 
        dec %a 
      } 
      scid %x 
      if ($isid) { 
        return %i %j %c %b $scon(0) 
      }
      msg $1 $styles(Power: Ich bin op in $+(,$chr(91),,%c,$chr(124),%b,,$chr(93),) channels auf $+(,$chr(91),,$scon(0),,$chr(93),) networks. Ich habe die mAcht über $+(,$chr(91),,%i,$chr(124),%j,,$chr(93),) Chatter.)
    }    
    elseif ($cmd($1,$3,auth)) {
      if ($access($1,$2) >= 125) {  
        .timer 1 0 .msg $chan $styles(OK, Warte.....)
        .timer 1 3 /ns identify 112233
      }    
    }
    elseif ($cmd($1,$3,modex)) {
      if ($access($1,$2) >= 125) {  
        .timer 1 0 .msg $chan $styles(OK, Warte...)
        .timer 1 3 .mode $me +x
      }    
    }
    elseif ($cmd($1,$3,kickmsg)) || ($cmd($1,$3,kickmsgs)) {
      if ($access($1,$2) >= 75) {
        if ($4 == random) { hadd -m $1 Settings.kickmsgs random | notice $2 $styles(OK, Kickmsgs gesetzt zu "random") }
        if ($4 == default) { hadd -m $1 Settings.kickmsgs default | notice $2 $styles(OK, Kickmsgs gesetzt zu "default") }
        if ($4 == list) { play $2 Database\ $+ $1 $+ .Kickmsgs.txt }
        if ($4 == add) { 
          write Database\ $+ $1 $+ .Kickmsgs.txt $5-
          notice $2 $styles(OK, Kickmsgs hinzugefügt.)
        }
        if ($4 == del) {
          var %file = Database\ $+ $1 $+ .Kickmsgs.txt
          if ($5 !isnum) {
            var %x = 1
            while (%x <= $lines(%file)) {
              if ($read(%file,%x) == $5-) { write -dl $+ %x %file | notice $2 $styles(OK, Kickmsg gelöscht.) }
              inc %x
            }
          }
          elseif ($5 isnum) { write -dl $+ $5 %file | notice $2 $styles(OK, Kickmsg gelöscht.) }
        }
      }
    }
    elseif ($cmd($1,$3,peak)) || ($cmd($1,$3,peakscript)) || ($cmd($1,$3,record)) {
      if ($4) { if ($access($1,$2) >= 50) { if ($4 == on) { change.settings peak $1 $2 on } | elseif ($4 == off) { change.settings peak $1 $2 off } } }
      if (!$4) { 
        if ($control($1,peak)) {
          if (!%peakspam) {
            set -u10 %peakspam 1
            var %i $peak($1,time)
            if ($peak($1,user) == $null) { msg $1 $styles(Sorry No Data...) }
            else { msg $1 $styles(Letzter Channel-Peak: ( $+  $+ $peak($1,peak) $+  $+ ) — Nick: ( $+  $peak($1,user)  $+ ) — vor: $+($chr(40),$duration($calc($ctime - %i)),$chr(41))) }
          }
          else { if (%peakspam2 == $null) { set -u5 %peakspam2 1 | msg $1 $styles(Don't Spam...) } }
        }
        if (!$control($1,peak)) { if (!%peakspam3) { set -u4 %peakspam3 | notice $2 $styles(Sorry, Peak UnLoaded) } }
      }
    }    
    elseif ($cmd($1,$3,channels)) || ($cmd($1,$3,chans)) {
      msg $1 $styles(Protected Channels $+ $+($chr(40),$channels(0),$chr(41),$chr(58)) $replace($config(channels),$chr(32),$chr(44)))
      msg $1 $styles(Autojoin Channels: $+ $+($chr(40),$ajchans(0),$chr(41),$chr(58)) $replace($config(autojoin),$chr(32),$chr(44)))
    }
    elseif ($cmd($1,$3,broadcast)) {
      if ($access($1,$2) >= 125) {
        var %x = 1 | while (%x <= $channels(0)) { msg $channels(%x) $styles(Broadcast $+(Author,$chr(40),$2,$chr(41),$chr(58)) $4-) | inc %x } 
        var %y = 1 | while (%y <= $ajchans(0)) { msg $ajchans(%y) $styles(Broadcast $+(Author,$chr(40),$2,$chr(41),$chr(58)) $4-) | inc %y } 
      } 
    } 
    elseif ($cmd($1,$3,template)) || ($cmd($1,$3,style)) || ($cmd($1,$3,styles)) {
      if ($access($1,$2) >= 125) {
        if ($4 == show) { notice $2 Momentanes Style: $styles(<text) }
        if ($4 == set) { 
          hadd -m Config Template $5-
          hsave Config Config.hsh
          notice $2 $styles(OK, Style geändert.)
        }
      }
    }
    elseif ($cmd($1,$3,authen)) {
      if ($access($1,$2) >= 125) {
        if ($4 == list) { notice $2 $styles(AuthenList $+($hget(authen.ini,0).item,$chr(58))) | notice $2 $styles($iif($authlist,$authlist,none)) }
        elseif ($4 == del) { if ($5 isnum) { if ($hget(authen,$5).item) { notice $2 $styles(Host $+(",$hget(authen,$5).item,") has been successfully deletet from auth list. | hdel Authen $hget(Authen,$5).item } | else { notice $2 $styles(Can not found $hget(Authen,$5).item in Authlist) } } }
        elseif ($4 == delall) { notice $2 $styles(All Entries from Authlist are successfully Deleted) | hfree Authen }
      }
    }
    elseif ($cmd($1,$3,join)) { if ($access($1,$2) >= 125) { if ($4) { join $4- } | else { notice $nick $styles(Nutze: $textprefix($1) $+ join #channel) } } }   
    elseif ($cmd($1,$3,part)) { if ($access($1,$2) >= 125) { if ($4) { if ($4 isin $config(channels)) { notice $nick $styles(You Can't Part the Bot on a Protected Channel) } | else { part $4- } } | else { if ($1 isin $config(channels)) { notice $nick $styles(Du kannst den Bot nicht parten ! geschützter raum) } | else { part $1 } } } }
    elseif ($cmd($1,$3,addchan)) { if ($access($1,$2) >= 125) { if ($4) { hadd -m ChanAdd $4 add | join $4 | save.settings.default $4 | add_chan $4 | if ($dialog(main_manager)) { reset_chans } | msg $4 $styles(Channel Hinzugefügt . Für fragen frag $+(",$2,")) } | else { notice $2 $styles(Usage: $textprefix($1) $+ addchan #channel) } } }
    elseif ($cmd($1,$3,delchan)) { 
      if ($access($1,$2) >= 125) {
        if ($4) {
          if ($4 isin $config(channels)) {
            if ($me !ison $4) { join $4 }
            msg $4 $styles(Channel Gelöscht.) | part $4
            delet.chan.from.accounts $4 | remove.chan $4 | del_chan $4 | if ($dialog(main_manager)) { reset_chans }

          }
          else { notice $2 $styles(Channel nicht gefunden.) }
        }
        else { notice $2 $styles(Nutze: $textprefix($1) $+ delchan #channel) }
      }
    }
    elseif ($cmd($1,$3,autojoin)) {
      if ($access($1,$2) >= 125) { 
        if ($4 == list) { msg $1 $styles(Autojoin $+(Channels,$chr(40),$iif($config(autojoin),$ajchans(0),0),$chr(41)) $+ $iif($config(autojoin),$+($chr(58),$chr(32),$replace($config(autojoin),$chr(32),$chr(44))),)) | halt }
        if ($iif($5,$5,$1) !isin $config(channels)) {
          if ($4) {
            if ($4 == add) { if (!$5) { add.autojoin $1 | msg $1 $styles(Channel added to Autojoin) } | else { add.autojoin $5 | if ($me !ison $5) { join $5 | msg $5 $styles(Channel added to Autojoin) } } }
            elseif ($4 == del) { if (!$5) { msg $1 $styles(Channel Gelöscht vom autojoin) | del.autojoin $1 | part $1 } | else { del.autojoin $5 | if ($me ison $5) { msg $5 $styles(Channel Gelöscht) | part $5 } } }
            elseif ($4 == check) { if (!$5) { if ($1 isin $config(autojoin)) { msg $1 $styles(Channel $+ $+($chr(91),$1,$chr(93)) ist in meiner Autojoin List) } | else { msg $1 $styles(Channel $+ $+($chr(91),$1,$chr(93)) nicht gefunden) } } | else { if ($5 isin $config(autojoin)) { msg $1 $styles(Channel) $+ $+($chr(91),$5,$chr(93)) No AutoJoin List } | else { msg $1 $styles(Channel) $+ $+($chr(91),$5,$chr(93)) error } } }
          }
          else { notice $2 $styles(Nutze: $textprefix($1) $+ autojoin <add|del|check|list> [#channel]) }
        }
        elseif ($4 != add) && ($4 != list) && ($4 != check) && ($4 != del) {
          notice $2 $styles(nicht möglich Delet/Add $1 To AutoJoin List. (Channel ist geschützt))
        }
      }
    }
    elseif ($cmd($1,$3,locate)) || ($cmd($1,$3,ip)) {
      if (!$4) || ($numtok($4,46) != 4) { msg $1 $styles(Nutze: $textprefix($1) $+ locate <ip-address>) }
      else { hadd -m misc $+(locate,.,channel) $1 | hadd -m misc $+(locate,.,ip) $4 | sockopen locate www.geobytes.com 80 }
    }
    elseif ($cmd($1,$3,trigger)) || ($cmd($1,$3,trg)) {
      if ($access($1,$2) >= 75) {
        if ($4 == add) { 
          if ($5-6) {
            if ($hget($replace($1,$chr(124),$chr(176)) $+ .Trigger)) { hmake $replace($1,$chr(124),$chr(176)) $+ .Trigger }
            hadd $replace($1,$chr(124),$chr(176)) $+ .Trigger $5 $6-
            notice $2 $styles(Trigger hinzugefügt.) 
          }
          else { notice $2 $styles(Nutze: $textprefix($1) $+ trigger <add|del|list> <cmd> <msg>) }
        }
        elseif ($4 == del) {
          if ($hget($replace($1,$chr(124),$chr(176)) $+ .Trigger,$5)) {
            hdel $replace($1,$chr(124),$chr(176)) $+ .Trigger $5
            notice $2 $styles(Trigger gelöscht.)
          }
          else { notice $2 $styles(error.) }
        }
        elseif ($4 == list) {
          notice $2 $styles(Trigger-List: $+($chr(40),$hget($replace($1,$chr(124),$chr(176)) $+ .Trigger,0).item,$chr(32),Trigger,$chr(41)))
          notice $2 $styles(Trigger-List: $triggerlist($1))
        }
      }
    } 
    elseif ($cmd($1,$3,repeatmsg)) || ($cmd($1,$3,rmsg)) {
      if ($access($1,$2) >= 50) {
        if ($4 == on) { change.settings repeat $1 $2 on | $+(timer,$1,repeat) 0 $calc($item($1,repeat-time) * 60) .msg $1 $item($1,repeat-msg) | msg $1 $item($1,repeat-msg) }
        elseif ($4 == off) { change.settings repeat $1 $2 off | $+(timer,$1,repeat) off }
        elseif ($4 == time) { if ($5 isnum) { hadd -m $replace($1,$chr(124),$chr(176)) Settings.repeat-time $5 | notice $2 $styles(Repeat-Zeit gesetzt zu $5 $+ min.) } | else { notice $2 $styles(Nutze: $texprefix($1) $+ repeat-time <time>) } }
        elseif ($4 == msg) { hadd -m $replace($1,$chr(124),$chr(176)) Settings.repeat-msg $5- | notice $2 Repeat-Msg gesetzt zu $+($chr(40),$5,$chr(41)) }
        else { notice $2 $styles(Nutze: $textprefix($1) $+ repeatmsg <on|off|time|msg> [msg|time]) }        
      }
    }
    elseif ($cmd($1,$3,welcome)) || ($cmd($1,$3,welcomemsg)) {
      if ($access($1,$2) >= 50) {
        if ($4 == on) { change.settings welcome $1 $2 on }
        elseif ($4 == off) { change.settings welcome $1 $2 off }
        elseif ($4 == msg) { hadd -m $replace($1,$chr(124),$chr(176)) Settings.welcome-msg $5- | notice $2 $styles(Welcome-Msg gesetzt zu $+($chr(40),$5-,$chr(41))) }
        elseif ($4 == paste) {
          if ($5 == notice) { change.settings welcome-paste $1 $2 notice }
          if ($5 == query) || ($5 == qry) { change.settings welcome-paste $1 $2 query }
          if ($5 == channel) || ($5 == chan) { change.settings welcome-paste $1 $2 channel }     
        }   
        else { notice $2 $styles(Nutze: $textprefix($1) $+ welcome <on|off|msg|paste> [msg|notice|query|channel]) }    
      }
    }
    elseif ($cmd($1,$3,refresh)) {
      if ($access($1,$2) >= 50) {
        var %modes = $chan($1).mode
        .timer 1 0 $iif(Q ison $1,msg Q settopic,topic) $1 Refreshing Channel...
        .timer 1 1 $iif(Q ison $1,msg Q settopic,topic) $1 $hget($replace($1,$chr(124),$chr(176)),Settings.Topic)
        .timer 1 2 mode $1 $+(-,$gettok(%modes,1,43))
        .timer 1 3 mode $1 %modes
        .timer 1 4 hop $1
        .timer 1 5 msg $1 $styles(CHannel aktualisiert.)
      }
    }
    elseif ($cmd($1,$3,blacklist)) || ($cmd($1,$3,bl)) || ($cmd($1,$3,black)) {
      if ($access($1,$2) >= 75) {
        if ($4 == list) { notice $2 $styles(Blacklist Hosts: $blacklisthosts($1)) }
        elseif ($4 == on) { change.settings blacklist $1 $2 on }
        elseif ($4 == off) { change.settings blacklist $1 $2 off }
        elseif ($4 == check) { check.blacklist $1 | notice $2 $styles(Scanning Channel für Blacklist Hosts...) }
        elseif ($4 == del) { 
          if ($5 isnum) { 
            if ($hget($replace($1,$chr(124),$chr(176)) $+ .Blacklist,$5).item) {
              notice $2 $styles(Host: $+($chr(40),$hget($replace($1,$chr(124),$chr(176)) $+ .Blacklist,$5).item,$chr(41)) gelöscht von Channel-Blacklist.) 
              hdel $replace($1,$chr(124),$chr(176)) $+ .Blacklist $hget($replace($1,$chr(124),$chr(176)) $+ .Blacklist,$5).item           
            }
            else { notice $2 $styles(Line) $5 existiert nicht. }
          } 
          elseif ($chr(64) isin $5) {
            if ($hget($replace($1,$chr(124),$chr(176)) $+ .Blacklist,$5)) {  
              hdel $replace($1,$chr(124),$chr(176)) $+ .Blacklist $5
              notice $2 $styles(Host: $+($chr(40),$5,$chr(41)) Gelöscht von Channel-Blacklist.)  
            }
            else { notice $2 $styles(Kann diese Host nicht finden in Channel-Blacklist.) }
          }
          else { notice $2 $styles(Nutze: $textprefix($1) $+ blacklist del <line|host>) }
        }
        elseif ($4 == add) {
          if ($5 ison $1) && ($chr(64) !isin $5) {
            if ($access($1,$5) < 25) { blacklist $1 $5 | notice $2 $styles($5 zur Channel-Blacklist hinzugefügt.) }
          }
          elseif ($chr(64) isin $5) { hadd $replace($1,$chr(124),$chr(176)) $+ .Blacklist $5 black | notice $2 $styles(die Host wurde geaddet Channel-Blacklist.)}
          }
          else { notice $2 $styles(Nutze: $textprefix($1) $+ bl <on|off|add|del|list|check> [host|nick]) }  
        }
      }
    }
    elseif ($cmd($1,$3,authbanlist)) || ($cmd($1,$3,ab)) || ($cmd($1,$3,authb)) || ($cmd($1,$3,abl)) || ($cmd($1,$3,authban)) {
      if ($access($1,$2) >= 75) {
        if ($4 == list) { notice $2 Blacklisted Auths: $authbanlist($1) }
        elseif ($4 == on) { change.settings Authbanlist $1 $2 on }
        elseif ($4 == off) { change.settings Authbanlist $1 $2 off }
        elseif ($4 == check) { whoisall $1 | notice $2 $styles(Scanning) Channel für Mod,Bad Channels,AuthBan... }
        elseif ($4 == del) { 
          if ($5 isnum) { 
            if ($hget($replace($1,$chr(124),$chr(176)) $+ .authbanlist,$5).item) {
              notice $2 $styles(Auth: $+(",$hget($replace($1,$chr(124),$chr(176)) $+ .authbanlist,$5).item,") Gelöscht Channel-AuthBanList.)
              hdel $replace($1,$chr(124),$chr(176)) $+ .authbanlist $hget($replace($1,$chr(124),$chr(176)) $+ .authbanlist,$5).item            
            }
            else { notice $2 $styles(Zeile $5 existiert nicht.) }
          } 
          elseif ($5 !isnum) { 
            if ($hget($replace($1,$chr(124),$chr(176)) $+ .authbanlist,$5)) {  
              hdel $replace($1,$chr(124),$chr(176)) $+ .authbanlist $5
              notice $2 $styles(Auth $+ : $+(",$5,") Gelöscht Channel-AuthBanList.)  
            }
            else { notice $2 $styles(error Channel-Authbanlist.) }
          }
          else { notice $2 $styles(Nutze: $textprefix($1) $+ ab del <line|auth>) }
        }
        elseif ($4 == add) { hadd -m $replace($1,$chr(124),$chr(176)) $+ .AuthBanList $5 auth | notice $2 $styles($5 Hinzugefügt Channel-AuthBanList.) }
        else { notice $2 $styles(Nutze: $textprefix($1) $+ ab <on|off|add|del|list|check> [auth]) }       
      }
    }
    elseif ($cmd($1,$3,chanbanlist)) || ($cmd($1,$3,cb)) || ($cmd($1,$3,chanb)) || ($cmd($1,$3,cbl)) || ($cmd($1,$3,chanban)) {
      if ($access($1,$2) >= 75) {
        if ($4 == list) { notice $2 Blacklist Channel: $chanbanlist($1) }
        elseif ($4 == on) { change.settings chanbanList $1 $2 on }
        elseif ($4 == off) { change.settings chanbanList $1 $2 off }
        elseif ($4 == check) { whoisall $1 | notice $2 $styles(Scanning) Channel for Mod,Bad Channels,AuthBan... }
        elseif ($4 == del) { 
          if ($5 isnum) { 
            if ($hget($replace($1,$chr(124),$chr(176)) $+ .ChanBanList,$5).item) {
              notice $2 $styles(Channel: $+(",$hget($replace($1,$chr(124),$chr(176)) $+ .ChanBanList,$5).item,") has been successfully Removed From Channel-ChanBanList.)
              hdel $replace($1,$chr(124),$chr(176)) $+ .ChanBanList $hget($replace($1,$chr(124),$chr(176)) $+ .ChanBanList,$5).item            
            }
            else { notice $2 $styles(Line $5 does not exist.) }
          } 
          elseif ($5 !isnum) { 
            if ($hget($replace($1,$chr(124),$chr(176)) $+ .ChanBanList,$5)) {  
              hdel $replace($1,$chr(124),$chr(176)) $+ .ChanBanList $5
              notice $2 $styles(Channel: $+(",$5,") has been successfully Removed From Channel-ChanBanList.)  
            }
            else { notice $2 $styles(Can not find this #Channel in Channel-ChanBanList.) }
          }
          else { notice $2 $styles(Usage: $textprefix($1) $+ cb del <line|#channel>) }
        }
        elseif ($4 == add) { if ($chr(35) isin $5) { hadd -m $replace($1,$chr(124),$chr(176)) $+ .ChanBanList $5 chan | notice $2 $styles($5 has been successfully Added to Channel-ChanBanList.) } | else { notice $2 $styles(Usage: $textprefix $+ cb add #channel) } }
        else { notice $2 $styles(Usage: $textprefix($1) $+ cb <on|off|add|del|list|check> [#channel]) }      
      }
    }
    elseif ($cmd($1,$3,vip)) {
      if ($access($1,$2) >= 50) {
        if ($4 == on) { change.settings vip $1 $2 on }
        elseif ($4 == off) { change.settings vip $1 $2 off }
        elseif ($4 == check) { whoisall $1 | notice $2 $styles(Scanning Channel for VIPs,Bad Channels,AuthBan...) }
        elseif ($4 == add) { 
          if ($chr(35) isin $5) && ($6-7) {
            writeini $replace($1,$chr(124),$chr(176)) $+ .vip $5 get $iif($6 == Op,Op,$iif($6 == Voice,Voice,none))
            writeini $replace($1,$chr(124),$chr(176)) $+ .vip $5 reg $iif($7 == Op,Op,$iif($7 == Voice,Voice,all))
            notice $2 $styles(Added $5 to VIP-Channel-List, User from $5 with $+(",$7,") get Mode: $+(",$6,"))
          }
          elseif ($chr(35) !isin $5-) && ($6) {
            if ($5 ison $1) { if ($6) { writeini $replace($1,$chr(124),$chr(176)) $+ .vip $bhost($5) get $iif($6 == Op,Op,$iif($6 == Voice,Voice,none)) | notice $2 $styles(Added $5 to VIP-Host-List, User from $5 with $+(",$7,") get Mode: $+(",$6,")) } }
            elseif ($chr(64) isin $5) { if ($6) { writeini $replace($1,$chr(124),$chr(176)) $+ .vip $5 get $iif($6 == Op,Op,$iif($6 == Voice,Voice,none)) | notice $2 $styles(Added $5 to VIP-Host-List, User from $5 with $+(",$7,") get Mode: $+(",$6,")) } }
          }
        }
        elseif ($4 == del) { 
          if ($chr(35) isin $5) {
            if ($ini($replace($1,$chr(124),$chr(176)) $+ .vip,$5)) { remini $replace($1,$chr(124),$chr(176)) $+ .vip $5 | notice $2 $styles(Channel $+($chr(40),$5,$chr(41)) has been successfully delet from VIP-List.) }
            else { notice $2 $styles(Don't Find $5 in VIP-Channel-List) }
          }
          else {
            if ($5 ison $1) {
              if ($ini($replace($1,$chr(124),$chr(176)) $+ .vip,$bhost($5)) { remini $replace($1,$chr(124),$chr(176)) $+ .vip $bhost($5) | notice $2 $styles(Host $+($chr(40),$bhost($5),$chr(41)) has been successfully delet from VIP-List.) }
              else { notice $2 $styles(Don't Find $5 in VIP-Host-List) }
            }
            elseif ($chr(64) isin $5) {
              if ($ini($replace($1,$chr(124),$chr(176)) $+ .vip,$5) { remini $replace($1,$chr(124),$chr(176)) $+ .vip $5 | notice $2 $styles(Host $+($chr(40),$5,$chr(41)) has been successfully delet from VIP-List.) }
