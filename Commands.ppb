        elseif ($2 != $hget($+(bombe,$1),nick)) { notice $2 Sry, es ist $hget($+(bombe,$1),nick) beim Bombenentschärfen }
      }
      elseif (!$hget($+(bombe,$1),bomb)) { notice $2 Die Bombe tickt nicht! Gib erst !bombe ein }
    }
    elseif ($cmd($1,$3,afraid)) || ($cmd($1,$3,fear)) {
      if ($hget($+(bombe,$1),bomb)) {
        if ($2 == $hget($+(bombe,$1),nick)) {
          if ($access($1,$2) >= 25) { msg $1 $styles(OMG it is only a virtuell bomb (no Kick for User)) }
          else { PPB_kick $1 $2 Kicked; Reason: Don't Be afraid, it is only a virtuell bomb }
          stop.bomb $1
        }
        elseif ($2 != $hget($+(bombe,$1),nick)) { notice $2 Sry, es ist $hget($+(bombe,$1),nick) beim Bombenentschärfen }
      }
      elseif (!$hget($+(bombe,$1),bomb)) { notice $2 Die Bombe tickt nicht! Gib erst !bombe ein }
    }
    elseif ($cmd($1,$3,request)) || ($cmd($1,$3,botrequest)) || ($cmd($1,$3,requestbot)) {
      if ($4) {
        if ($4 == on) || ($4 == off) {
          if ($access($1,$2) >= 125) {
            if ($4 == on) { hadd -m Config Misc.req-status on | notice $2 $styles(Request is now Loaded.) }
            elseif ($4 == off) { hadd -m Config Misc.req-status off | notice $2 $styles(Request is now Unloaded.) }
          }
        }
        elseif ($4 == idler) { 
          if ($access($1,$2) >= 125) {
            if (!$5) { notice $2 $styles(Request: Min.Idler are set on $+($chr(40),$config(req-idler),$chr(41))) }
            else { hadd -m Config Misc.req-idler $5 | notice $2 $styles(Request: Min.Idler are NOW set on $+($chr(40),$config(req-idler),$chr(41))) }
          }
        }
        elseif ($4 == help) { notice $2 $styles(Min.-Idler: $config(req-idler) ; Op in Request-Channel) | notice $2 $styles(Usage:) $textprefix($1) $+ request <#channel> }
        elseif ($left($4,1) == $chr(35)) {
          if ($config(req-status) == on) { 
            if (%requestspamm == 1) {
              if (!%requestspamm2) {
                msg $2 Please $styles(Sry Try later ; A another request is on Work! (Try again in ca. 15 secs))
                set -u10 %requestspamm2 1
              }
            }
            if (!%requestspamm) {
              set -u10 %requestspamm 1
              set -u40 %rn. [ $+ [ $4 ] ] $2
              if ($4 != #0,0) {
                set %m.chans $chan(0)
                set %rchan. [ $+ [ $4 ] ] $4
                if (%m.chans < 19) {
                  notice $2 $styles(Please wait a moment. Your request is in work!)
                  join $4
                  .timer 1 2 chanidler $4
                  .timer 1 3 checkidler $4 $2
                  .timer 1 5 unset %rn. [ $+ [ $4 ] ]
                  .timer 1 5 unset %idler1. [ $+ [ $4 ] ] 
                  .timer 1 5 unset %idler2. [ $+ [ $4 ] ]
                  .timer 1 5 unset %rchan. [ $+ [ $4 ] ]
                  .timer 1 5 unset %m.chans
                }
                else { notice $2 $styles(Sry But my Protectclass is full! U cant request ME!) | unset %m.chans } 
              }
            }
          }
          elseif ($config(req-status) == off) { if (!%requestspam3) { set -u5 %requestspam3 1 | notice $2 $styles(Request Unloaded.) } }
        }
        else { if (!%requstspam3) { set -u5 %requestspam3 1 | notice $2 $styles(Usage: !request <on|off|#channel|idler> [idler]) } }
      }
    }
    elseif ($trigger($1,$3)) { msg $1 $styles($trigger($1,$3)) }
  }
}
