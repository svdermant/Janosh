on *:TEXT:!op's:#: {
  msg # 14,1 [15,1:14,1·12,01 $+  # $+ 14,1·15,1:14,1] 11,1h10at  9•4,1 $nick(#,0,o) 9• 11O10peratoren
  if ($nick(#,0,o) == 0) {
    msg # 4 14]7:::14[4Es sind keine op´s in # Zu Sehen14]7:::14[
  }
  if ($nick(#,0,o) > 0 ) {
    set %ops $nick(#,0,o)
    :next 
    msg #  14,1 ]3:9:3:14[4 $nick(#,%ops,o) 14]3:9:3:14[
    dec %ops
    if %ops == 0 goto done
    else goto next
    :done
  }
}
